{
  "name": "ember-cli-typescript-blueprints",
  "version": "3.0.0",
  "description": "TypeScript blueprints for generating ember-cli entities",
  "keywords": [
    "ember-addon"
  ],
  "repository": "typed-ember/ember-cli-typescript-blueprints",
  "license": "MIT",
  "author": "The typed-ember team (https://github.com/typed-ember)",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "lint": "npm-run-all --continue-on-error --aggregate-output --parallel \"lint:!(fix)\"",
    "lint:eslint": "eslint --report-unused-disable-directives --cache .",
    "lint:eslint:fix": "npm-run-all \"lint:eslint --fix\"",
    "lint:fix": "npm-run-all lint:*:fix",
    "test": "mocha node-tests/blueprints/**/*-test.js"
  },
  "dependencies": {
    "@ember/edition-utils": "^1.2.0",
    "chalk": "^4.0.0",
    "ember-cli-babel": "^7.23.0",
    "ember-cli-get-component-path-option": "^1.0.0",
    "ember-cli-is-package-missing": "^1.0.0",
    "ember-cli-normalize-entity-name": "^1.0.0",
    "ember-cli-path-utils": "^1.0.0",
    "ember-cli-string-utils": "^1.1.0",
    "ember-cli-test-info": "^1.0.0",
    "ember-cli-valid-component-name": "^1.0.0",
    "ember-cli-version-checker": "^5.1.1",
    "ember-router-generator": "^2.0.0",
    "exists-sync": "^0.1.0",
    "inflection": "^1.12.0",
    "semver": "^7.3.5",
    "silent-error": "^1.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@typed-ember/renovate-config": "1.2.1",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "broccoli-asset-rev": "3.0.0",
    "ember-ajax": "5.0.0",
    "ember-cli": "3.12.0",
    "ember-cli-blueprint-test-helpers": "0.19.2",
    "ember-cli-dependency-checker": "3.2.0",
    "ember-cli-eslint": "5.1.0",
    "ember-cli-inject-live-reload": "2.0.2",
    "ember-cli-shims": "1.2.0",
    "ember-cli-sri": "2.1.1",
    "ember-cli-uglify": "3.0.0",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-export-application-global": "2.0.1",
    "ember-load-initializers": "2.1.2",
    "ember-maybe-import-regenerator": "0.1.6",
    "ember-qunit": "4.6.0",
    "ember-resolver": "8.0.2",
    "ember-source": "3.28.8",
    "ember-source-channel-url": "3.0.0",
    "ember-try": "1.4.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-disable-features": "^0.1.3",
    "eslint-plugin-ember": "^10.5.8",
    "eslint-plugin-ember-internal": "^2.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-qunit": "^7.2.0",
    "fs-extra": "^10.0.0",
    "loader.js": "4.7.0",
    "mocha": "7.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "qunit-dom": "1.6.0",
    "testdouble": "3.16.1",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "after": [
      "ember-source",
      "ember-data",
      "ember-decorators"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 2,
    "proseWrap": "never"
  },
  "volta": {
    "node": "14.18.0"
  }
}
